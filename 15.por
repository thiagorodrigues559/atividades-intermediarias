programa {
  funcao inicio() {
    inteiro x , i
    escreva("DIGITE UM NUMERO  : \n" )
    leia(x)

    escreva(" a potencia de ", x, " e igual: ")

    para (i= 0; i <=10; i++)
    se (x == x){
      faca{
        i= x*x*x*x
        escreva(i)

      }enquanto (x == 0)
      escreva("\n")
    }

  }

}
